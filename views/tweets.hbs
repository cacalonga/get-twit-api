<div class="container">
    <div class="jumbotron">

        <table class="table">
            <h2>Tweets</h2>
            <thead>
                <tr>
                    <th scope="col">count</th>
                    <th scope="col">id</th>
                    <th scope="col">Created_at</th>
                    <th scope="col">name</th>
                    <th scope="col">Screen_name</th>
                    <th scope="col">Tweet</th>

                </tr>
            </thead>
            <tbody>
                {{#each tweetList as |tw key|}}
                <tr>
                    <td>{{key}}</td>
                    <td>{{tw.id}}</td>
                    <td>{{tw.created_at}}</td>
                    <td>{{tw.name}}</td>
                    <td>{{tw.screen_name}}</td>
                    <td>{{tw.text}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>

    </div>
</div>
